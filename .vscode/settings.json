  "files.exclude": {
    ".hugo_build.lock": true,
    "public/": true,
    "resources/": true,
    "design-files": true
  },
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    ".gitignore": ".gitattributes",
    "hugo.yaml": ".markdownlint.json, go.mod, go.sum, .coderabbit.yml",
    "eslint.config.js": ".markdownlint.jsonc",
